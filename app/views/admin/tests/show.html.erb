<h3><%= t('.test') %> <%= @test.title %>. </h3>
<% if @test.questions.any? %>
  <h4><%= t('.questions') %></h4>
  <% @test.questions.each do |question| %>
  <p><%= question.body %></p>
  <% end %>
  <% end %>
<%= link_to t('.tests'), admin_tests_path %>
<%= link_to t('.new test'), new_admin_test_path %>
