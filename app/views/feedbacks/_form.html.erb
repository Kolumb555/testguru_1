<%= form_with model: @feedback, local: true do |form| %>
<%= render 'shared/errors', resource: @feedback %>

  <p>
    <%= form.label t('.subject') %><br>
    <%= form.text_field :subject %>
  </p>

  <p>
    <%= form.label t('.comment') %><br>
    <%= form.text_area :comment %>
  </p>

<p><%= form.submit t('.send') %></p>

<% end %>
